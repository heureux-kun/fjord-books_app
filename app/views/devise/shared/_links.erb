<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to User.human_attribute_name(:signup), new_registration_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to t('link.forget-your-password'), new_password_path(resource_name) %><br />
<% end %>